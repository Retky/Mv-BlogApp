<h1> List of Users<h1>
<ul class="usersL">
  <% @users.each do |user| %>
    <div class="user">
      <div class="userPhoto"><%=image_tag user.photo %></div>
      <div class="name">
        <h3><%= user.name %></h3>
        <small>Number of posts: <%= user.posts_counter %></small>
      </div>
    </div>
  <% end %>
</ul>
